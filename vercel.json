{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/public/Retrospectives/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/Retrospectives/(.*)",
      "destination": "/public/Retrospectives/$1"
    }
  ],
  "github": {
    "silent": true
  },
  "outputDirectory": ".next"
}