{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/Retrospectives/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000, immutable"
      }
    }
  ],
  "github": {
    "silent": true
  }
}